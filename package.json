{
  "name": "codecascades-solver",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:shahata/codecascades-solver.git"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "f(){ if [ -z \"$CI\" ]; then jest --onlyChanged $*; else jest $*; fi }; NODE_OPTIONS=--experimental-vm-modules f",
    "lint": "eslint src && tsc",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.1"
  },
  "prettier": {
    "quoteProps": "consistent",
    "arrowParens": "avoid"
  },
  "wallaby": {
    "autoDetect": true,
    "startAutomatically": true,
    "env": {
      "params": {
        "env": "NODE_OPTIONS=--experimental-vm-modules"
      }
    }
  }
}